// http://eslint.org/docs/user-guide/configuring
{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules":{
        "no-console": 0,
        "max-len": [
            "warn",
            {
                "code": 120,
                "ignoreTrailingComments": true,
                "ignoreUrls": true
            }
            ],
        "camelcase": "off",
        "object-curly-spacing": "off"
    }
}